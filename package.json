{
  "name": "es-query",
  "displayName": "ES Query",
  "description": "Query ElasticSearch from vscode",
  "version": "0.0.1",
  "publisher": "jitcoder",
  "images": "images/elasticsearch.png",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:esQuery.execute",
    "onCommand:esQuery.setHost"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "esQuery.execute",
        "title": "Execute ElasticSearch Query",
        "category": "ES"
      },
      {
        "command": "esQuery.setHost",
        "title": "Set ElasticSearch Host",
        "category": "ES"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "esQuery.execute"
        }
      ]
    },
    "keybindings": [{
      "command": "esQuery.execute",
      "key": "ctrl+shift+e",
      "mac": "cmd+shift+e"
    }]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "request": "^2.79.0"
  }
}
